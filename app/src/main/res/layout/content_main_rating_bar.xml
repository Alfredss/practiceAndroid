<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="mundo.hola.com.example.alfre.androidpractice.MainRatingBarActivity"
    tools:showIn="@layout/activity_main_rating_bar">

    <RatingBar
        android:id="@+id/rbRatingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/botonRatingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rating"/>
    <Button
        android:id="@+id/ratingBarBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/regresar"/>

</LinearLayout>
